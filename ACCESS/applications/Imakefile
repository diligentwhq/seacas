#define IHaveSubdirs

#define PassDebugFlags	"DEBUG=$(DEBUG)" \
	                "CDEBUGFLAGS=$(CDEBUGFLAGS)" \
	                "CXXDEBUGFLAGS=$(CXXDEBUGFLAGS)" \
			"F90DEBUGFLAGS=$(F90DEBUGFLAGS)" \
			"F77DEBUGFLAGS=$(F77DEBUGFLAGS)"

SUBDIRS = \
$(APREPROSRC) \
$(GJOINSRC) \
$(GROPESRC) \
$(MAPVARSRC) \
$(MAPVARKDSRC) \
$(ALGEBRASRC) \
$(CONJOINSRC) \
$(EJOINSRC) \
$(EX1EX2V2SRC) \
$(EXOTXTSRC) \
$(EXODIFFSRC) \
$(TXTEXOSRC) \
$(NEM_SLICESRC) \
$(NEM_SPREADSRC) \
$(EPUSRC) \
$(FONTSSRC) \
$(BLOTSRC)

MakeAllTargets($(SUBDIRS),PassDebugFlags)


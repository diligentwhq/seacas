CODE = sphgen3d

SRCS = sphgen3d.f

OBJS = ${SRCS:.f=.o}

LIBS = $(EXOIIV2FOR) $(EXOIIV2C) $(NETCDF) $(SUPLIB) $(SUPES) 

NormalFortranTarget($(CODE),$(OBJS),NullParameter,NullParameter,$(LIBS))

InstallTarget(install,$(CODE),$(INSTBINFLAGS),$(BINDIR))

DependTarget()
